#include <iostream>  
using namespace std;  
int main()  
{  
  int n, i, flag=0,sum=0, j;  
  cout << "Enter the Number to get sum of prime ";  
  cin >> n;  
for(j=2;j<=n;j++)
{
   flag=0;
    for(i = 2; i <= j/2; i++)  
  {  
      if(j % i == 0)  
      {   
          flag=1;  
          break;  
      }  
  }  
  if (flag==0)  
     {
       sum+=j;
     }
}  
cout<<"sum of the numbers is"<<endl<<sum;
  return 0;  
}  
